{% extends 'base.html.twig' %}

{% block title %}Competence index{% endblock %}

{% block body %}
    <h1>Competence index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CompBase</th>
                <th>CompDesk</th>
                <th>CompInternet</th>
                <th>CompEmail</th>
                <th>IsAutoEva</th>
                <th>IsDigComp1</th>
                <th>IsDigComp2</th>
                <th>IsDigComp3</th>
                <th>IsDigComp4</th>
                <th>IsDigComp5</th>
                <th>DetailParcour</th>
                <th>IsAutoEvalEnd</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for competence in competences %}
            <tr>
                <td>{{ competence.id }}</td>
                <td>{{ competence.compBase }}</td>
                <td>{{ competence.compDesk }}</td>
                <td>{{ competence.compInternet }}</td>
                <td>{{ competence.compEmail }}</td>
                <td>{{ competence.isAutoEva ? 'Yes' : 'No' }}</td>
                <td>{{ competence.isDigComp1 ? 'Yes' : 'No' }}</td>
                <td>{{ competence.isDigComp2 ? 'Yes' : 'No' }}</td>
                <td>{{ competence.isDigComp3 ? 'Yes' : 'No' }}</td>
                <td>{{ competence.isDigComp4 ? 'Yes' : 'No' }}</td>
                <td>{{ competence.isDigComp5 ? 'Yes' : 'No' }}</td>
                <td>{{ competence.detailParcour }}</td>
                <td>{{ competence.isAutoEvalEnd ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('app_gestapp_competence_show', {'id': competence.id}) }}">show</a>
                    <a href="{{ path('app_gestapp_competence_edit', {'id': competence.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="14">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_gestapp_competence_new') }}">Create new</a>
{% endblock %}
