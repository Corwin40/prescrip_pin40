{% extends 'base.html.twig' %}

{% block title %}Member{% endblock %}

{% block body %}
    <h1>Member</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ member.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ member.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ member.roles ? member.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ member.password }}</td>
            </tr>
            <tr>
                <th>NameStructure</th>
                <td>{{ member.nameStructure }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ member.address }}</td>
            </tr>
            <tr>
                <th>Zipcode</th>
                <td>{{ member.zipcode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ member.city }}</td>
            </tr>
            <tr>
                <th>ContactEmail</th>
                <td>{{ member.contactEmail }}</td>
            </tr>
            <tr>
                <th>ContactPhone</th>
                <td>{{ member.contactPhone }}</td>
            </tr>
            <tr>
                <th>ContactResponsableFirstname</th>
                <td>{{ member.ContactResponsableFirstname }}</td>
            </tr>
            <tr>
                <th>ContactResponsableLastname</th>
                <td>{{ member.contactResponsableLastname }}</td>
            </tr>
            <tr>
                <th>ContactResponsableCivility</th>
                <td>{{ member.contactResponsableCivility }}</td>
            </tr>
            <tr>
                <th>IsVerified</th>
                <td>{{ member.isVerified ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_member_index') }}">back to list</a>

    <a href="{{ path('app_admin_member_edit', {'id': member.id}) }}">edit</a>

    {{ include('admin/member/_delete_form.html.twig') }}
{% endblock %}
